/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    ListNode *getIntersectionNode(ListNode *headA, ListNode *headB) {
        unordered_set<ListNode*> headAS;
        ListNode* tmp = NULL;
        tmp = headA;
        while (tmp != NULL) {
            headAS.insert(tmp);
            tmp = tmp->next;
        }
        tmp = headB;
        while (tmp != NULL) {
            if (headAS.find(tmp) != headAS.end()) {
                return tmp;
            }
            tmp = tmp->next;
        }
        return NULL;
    }
};
