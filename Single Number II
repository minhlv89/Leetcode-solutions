class Solution {
public:
    int singleNumber(vector<int>& nums) {
        int result = 0;
        int table[32] = {};
        for (int i = 0; i < 32; i++) {
            int mask = (1 << i), sum = 0;
            for (int num : nums) {
                if (num & mask) sum ++;
            }
            if (sum % 3 != 0) result |= mask;
        }
        return result;
    }
};
